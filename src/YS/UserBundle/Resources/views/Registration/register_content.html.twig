{% trans_default_domain 'FOSUserBundle' %}
{% extends 'YSUserBundle::layout_login_content.html.twig'  %}
{% block login_content %}
    {{ form_start(form, {'method': 'post', 'action': path('fos_user_registration_register'), 'attr': {'class': 'fos_user_registration_register'}}) }}
        {{ form_widget(form) }}
        <div class="row">
            <div class="col s12">
                <input type="submit" class="btn col s12" value="Sign up" />
            </div>
        </div>
    {{ form_end(form) }}
{% endblock %}
{% block login_button %}
    <div class="row">
        <div class="col s12">
            <a class="btn col s12" href="{{ path('fos_user_security_login') }}">Sign in</a>
        </div>
    </div>
{% endblock %}
